# Block devices
/dev/block/bootdevice/by-name/persist		u:object_r:persist_block_device:s0
/dev/block/bootdevice/by-name/userdata          u:object_r:userdata_block_device:s0

# Data files
/data/misc/netmgr/log\.txt			u:object_r:netmgrd_data_file:s0

# Fpc Fingerprint
/data/fpc(/.*)?					u:object_r:fpc_data_file:s0
/dev/socket/fpce(/.*)?				u:object_r:fpce_socket:s0
/sys/devices/soc/soc:fpc1020(/.*)?		u:object_r:fpc_sysfs:s0

# For Goodix fingerprint
/dev/gf66xx-spi                                 u:object_r:goodixfpd_device:s0
/dev/ttyACM[0-9]*                               u:object_r:goodixfpd_device:s0
/dev/goodix_fp*                                 u:object_r:goodixfpd_device:s0
/system/bin/goodixfingerprintd                  u:object_r:goodixfpd_exec:s0

# Goodix Fingerprint data
/data/gf_data/frr_database.db                   u:object_r:goodixfpd_data_file:s0 
/persist/data/gf(/.*)?                          u:object_r:goodixfpd_data_file:s0

# Ir
/dev/peel_ir					u:object_r:peelir_device:s0
